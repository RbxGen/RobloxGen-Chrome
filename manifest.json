{
	"name": "RobloxGen",
    "description": "Chrome extension for RobloxGen",
	"version": "1.0",
	"manifest_version": 3,
	"background": {
		"service_worker": "background.js"
	},
	"action": {
		"default_popup": "popup.html"
	},
    "permissions": [
        "cookies"
    ],
	"host_permissions": [
        "*://*.roblox.com/"
    ],
    "options_page": "options.html"
}
